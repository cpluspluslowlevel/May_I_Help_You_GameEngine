{
    "version": 8,
    "configurePresets": [
        {
            "name": "Ninja_Build",
            "displayName": "Debug_Ninja_Clang",
            "description": "Build",
            "generator": "Ninja",
            "binaryDir": "${workspaceFolder}/Binary/${presetName}",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_TOOLCHAIN_FILE": "",
                "CMAKE_INSTALL_PREFIX": "${workspaceFolder}/Binary/${presetName}",
                "CMAKE_C_COMPILER": "D:/Development/LLVM/bin/clang.exe",
                "CMAKE_CXX_COMPILER": "D:/Development/LLVM/bin/clang++.exe"
            }
